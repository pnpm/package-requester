{
  "name": "@pnpm/package-requester",
  "version": "0.8.8",
  "description": "Concurrent downloader of npm-compatible packages",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "start": "npm run tsc -- --watch",
    "lint": "tslint -c tslint.json --project .",
    "tsc": "rimraf lib && tsc --pretty",
    "test": "npm run lint && preview && ts-node test --type-check && mos t",
    "md": "mos",
    "prepare": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pnpm/package-requester.git"
  },
  "keywords": [
    "pnpm",
    "resolver",
    "npm"
  ],
  "author": "Zoltan Kochan <z@kochan.io> (https://www.kochan.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/package-requester/issues"
  },
  "homepage": "https://github.com/pnpm/package-requester#readme",
  "peerDependencies": {
    "@pnpm/logger": "^1.0.0"
  },
  "dependencies": {
    "@pnpm/check-package": "^1.0.0",
    "@pnpm/fetcher-base": "^1.0.0",
    "@pnpm/pkgid-to-filename": "^1.0.0",
    "@pnpm/resolver-base": "^1.0.0",
    "@pnpm/types": "^1.3.0",
    "@types/load-json-file": "^2.0.7",
    "@types/mz": "^0.0.32",
    "@types/node": "^9.6.6",
    "@types/p-queue": "^2.3.1",
    "@types/write-json-file": "^2.2.1",
    "load-json-file": "^5.0.0",
    "mkdirp-promise": "^5.0.1",
    "mz": "^2.7.0",
    "p-limit": "^1.1.0",
    "p-queue": "^2.3.0",
    "path-exists": "^3.0.0",
    "read-package-json": "^2.0.12",
    "rename-overwrite": "^1.0.0",
    "rimraf-then": "^1.0.1",
    "symlink-dir": "^1.1.0",
    "util.promisify": "^1.0.0",
    "write-json-file": "^2.3.0"
  },
  "devDependencies": {
    "@pnpm/logger": "^1.0.0",
    "@pnpm/npm-resolver": "^0.3.3",
    "@pnpm/tarball-fetcher": "^0.4.4",
    "@types/nock": "^9.1.3",
    "@types/tape": "^4.2.31",
    "@types/tempy": "^0.1.0",
    "mos": "^2.0.0-alpha.3",
    "mos-plugin-readme": "^1.0.4",
    "nock": "^9.2.5",
    "package-preview": "^1.0.1",
    "rimraf": "^2.6.2",
    "tape": "^4.8.0",
    "tempy": "^0.2.1",
    "ts-node": "^6.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.1"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "publishConfig": {
    "scripts": {}
  }
}
